<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="TableBase">
    <defName>Gwanmuldae</defName>
    <label>Gwanmuldae (Korean-Style Military Locker)</label>
    <description>A gwanmuldae, or Korean-Style military locker. Neat and tidy!</description>
    <surfaceType>Item</surfaceType>
    <uiOrder>2900</uiOrder>
    <graphicData>
      <texPath>Things/Building/Furniture/Gwanmuldae</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3,3)</drawSize>
      <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
      </shadowData>
    </graphicData>
    <uiIconScale>0.65</uiIconScale>
    <uiIconOffset>(0, 0.15)</uiIconOffset>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>1</fillPercent>
    <minifiedDef>MinifiedThing</minifiedDef>
    <statBases>
      <MaxHitPoints>75</MaxHitPoints>
      <WorkToBuild>1000</WorkToBuild>
      <Mass>5</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>3</Beauty>
    </statBases>
    <pathCost>30</pathCost>
    <size>(1,1)</size>
    <defaultPlacingRot>South</defaultPlacingRot>
    <costStuffCount>30</costStuffCount>
    <placeWorkers>
      <li>PlaceWorker_ShowFacilitiesConnections</li>
    </placeWorkers>
    <researchPrerequisites>
      <li>ComplexFurniture</li>
    </researchPrerequisites>
    <comps>
      <li Class="CompProperties_Facility">
        <statOffsets>
          <Comfort>0.05</Comfort>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
        <mustBePlacedAdjacentCardinalToBedHead>true</mustBePlacedAdjacentCardinalToBedHead>
      </li>
    </comps>
    <staticSunShadowHeight>0</staticSunShadowHeight>
    <castEdgeShadows>false</castEdgeShadows>
  </ThingDef>

</Defs>

	<Vehicles.VehicleBuildDef ParentName="VehicleBaseBuildable">
		<defName>bicycle_blueprint</defName>
		<label>bicycle</label>
		<description>A bicycle.</description>
		<designationCategory>VF_Vehicles</designationCategory>
		<graphicData>
			<texPath>Things/Vehicles/bicycle/bicycle</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(1,1)</drawSize>
		</graphicData>

		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>350</WorkToBuild>
